<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PropertyManager.View.AddressView">
  <ContentView.Content>
      <StackLayout>
            <Entry Text="{Binding Street1, Mode=TwoWay}" Placeholder="Street 1"/>
            <Entry Text="{Binding Street2, Mode=TwoWay}" Placeholder="Street 2"/>
            <Entry Text="{Binding Town, Mode=TwoWay}" Placeholder="Town"/>
            <Entry Text="{Binding County, Mode=TwoWay}" Placeholder="County"/>
            <Entry Text="{Binding Postcode, Mode=TwoWay}" Placeholder="Postcode"/>
      </StackLayout>
  </ContentView.Content>
</ContentView>